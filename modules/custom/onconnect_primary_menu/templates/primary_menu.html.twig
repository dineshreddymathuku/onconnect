<div class="header-navigation">
  <div id="mobile-hamburger" class="mobile-hamburger">
      <img src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/mobile-hamburger.svg"
           alt="mobile-menu">
  </div>
    <nav class="clearfix">
    <ul class="clearfix header-menu">
      {% for id, menu in menus %}
        <li class="header-main-menu {{ menu.name|replace({' ':''}) }}"><a class="hover-menu-list">{{ menu.name }}</a>
          {% if menu.children %}
            <ul class="header-dropdown-menu row">
              <li class="dropdown-left col-lg-3">
			  
                <ul class="left-menu">
				    {% if menu.group_header %}
            {% for groupId, groupData in menu.group_header %}
		        {% if groupData.alias%}
		        <li class="group-name"><a href="{{ groupData.alias }}">{{ groupId }}</a> </li>
		          {% else %}
		        <li class="group-name">{{ groupId }} </li>
		        {% endif  %}
              {% if groupData.children %}
		            {% for childId, childData in groupData.children %}
                    <li class="{{ childData.name }}">
                      <a class="main-child latest-view-link " href="{{ childData.alias }}"
                         data-latestTitle="{{ menu.name }} > {{ childData.name }}">{{ childData.name }}
                         
                        {% if childData.sub_child %}
                          <span><img alt="right-angled-arrow-icon"
                                     src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/angle_right_arrow.svg"/></span>
                          <img class="blue-icon" alt="right-angled-arrow-icon"
                               src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/menu-blue-icon.svg"/>
                        {% endif %}
                      </a>
                      {% if childData.sub_child %}
                        <ul>
                          <li class="dropdown-right col-lg-9">
                            {% for subChildId, subChildData in childData.sub_child %}
                              <ul>
                                <li class="sub-child-details">
                                  <a class="latest-view-link" href="{{ subChildData.alias }}"
                                     data-latestTitle="{{ childData.name }} > {{ subChildData.name }}"><span
                                      class="sub-menu-title">{{ subChildData.name }}</span></a>
                                  {% if subChildData.description %}
								                  {% set description = subChildData.description|render|striptags %}
                                    <p class="description">{{ description|length > 80 ? description|slice(0, 80)|raw ~ '..' : description|raw }}</p>
                                  {% endif %}
                                  {% if subChildData.node %}
								 
                                    <a class="sub-child-node"
                                       href="{{ subChildData.node.alias }}">{{ subChildData.node.label }}</a>
                                  {% endif %}
                                </li>
                              </ul>
                            {% endfor %}
                            <ul>
                              {% if childData.view_all %}
                                <li class="view-all"><a href="{{ childData.view_all }}">View all <span><img
                                        alt="right-angled-arrow-icon"
                                        src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/right-arrow.svg"/></span></a>
                                </li>
                              {% endif %}
                              {% if childData.submit_publication %}
                                <li class="submit-publication submit"><a target="_blank"
                                                                         href="{{ childData.submit_publication }}">Submit
                                    Publication Concept <span><img
                                        alt="right-angled-arrow-icon"
                                        src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/submit-arrow.svg"/></span></a>
                                </li>
                              {% endif %}
                            </ul>
                          </li>
                        </ul>
                      {% endif %}
                    </li>
                  {% endfor %}
		            {% endif %}
		          {% endfor %}
		        {% endif %}
                  {% for childId, childData in menu.children %}
                    <li>
					
                      <a class="main-child latest-view-link {{ childData.active }} " href="{{ childData.alias }}"
                         data-latestTitle="{{ menu.name }} > {{ childData.name }}">{{ childData.name }}
                        {% if childData.sub_child %}
                          <span><img alt="right-angled-arrow-icon"
                                     src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/angle_right_arrow.svg"/></span>
                          <img class="blue-icon" alt="right-angled-arrow-icon"
                               src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/menu-blue-icon.svg"/>
                        {% endif %}
                      </a>
                      {% if childData.sub_child %}
                        <ul>
                          <li class="dropdown-right col-lg-9">
                            {% for subChildId, subChildData in childData.sub_child %}
                              <ul>
                                <li class="sub-child-details">
                                  <a class="latest-view-link" href="{{ subChildData.alias }}"
                                     data-latestTitle="{{ childData.name }} > {{ subChildData.name }}"><span
                                      class="sub-menu-title">{{ subChildData.name }}</span></a>
                                  {% if subChildData.description %}
								                  {% set description = subChildData.description|render|striptags %}
                                    <p class="description">{{ description|length > 80 ? description|slice(0, 80)|raw ~ '..' : description|raw }}</p>
                                  {% endif %}
                                  {% if subChildData.node %}
                                    <a class="sub-child-node"
                                       href="{{ subChildData.node.alias }}">{{ subChildData.node.label }}</a>
                                  {% endif %}
                                </li>
                              </ul>
                            {% endfor %}
                            <ul>
                              {% if childData.view_all %}
                                <li class="view-all"><a href="{{ childData.view_all }}">View all <span><img
                                        alt="right-angled-arrow-icon"
                                        src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/right-arrow.svg"/></span></a>
                                </li>
                              {% endif %}
                              {% if childData.submit_publication %}
                                <li class="submit-publication submit"><a target="_blank"
                                                                         href="{{ childData.submit_publication }}">Submit
                                    Publication Concept <span><img
                                        alt="right-angled-arrow-icon"
                                        src="/profiles/pfeconconnectpfcom_profile/themes/onconnect/images/icons/submit-arrow.svg"/></span></a>
                                </li>
                              {% endif %}
                            </ul>
                          </li>
                        </ul>
                      {% endif %}
                    </li>
                  {% endfor %}
                </ul>
              </li>
            </ul>
          {% endif %}
        </li>
      {% endfor %}
    </ul>
  </nav>
</div>
{# mobile-menu #}
<div id="mobile-popup" class="mobile-menu-popup">
  <nav>
    <ul class="mobile-menu">
      <li class="mobile-primary-menu">
        <ul>
          <li class="mobile-user-data">
            <span class="mobile-bookmark"><a href="/my/bibliograpy"><img alt="bookmark-icon"
