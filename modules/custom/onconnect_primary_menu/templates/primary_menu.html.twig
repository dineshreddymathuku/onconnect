                        {% endif %}
                        <a class="mobile-secondary-menu-parent" href="{{ childData.alias }}">{{ childData.name }}</a>
                        {% if childData.sub_child %}
                          <span class="mobile-right-arrow"><img alt="right-angled-arrow-icon"
